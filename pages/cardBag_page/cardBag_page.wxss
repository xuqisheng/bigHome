.containerCar {
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
}

.containerCar image {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}

.nav {
  width: 100%;
  height: 40px;
  display: flex;
  line-height: 40px;
  z-index: 999;
}

.default {
  line-height: 40px;
  text-align: center;
  flex: 1;
  border-right: 1px solid gainsboro;
  color: #000;
  font-weight: bold;
  font-size: 15px;
}

.red {
  line-height: 40px;
  text-align: center;
  color: #3aa1d5;
  flex: 1;
  border-right: 1px solid gainsboro;
  font-weight: bold;
  font-size: 15px;
}

.show {
  display: block;
  z-index: 999;
}

