.maps {
  position: relative;
}

.houses {
  width: 96%;
  height: 260px;
  background: #fff;
  z-index: 999;
  position: absolute;
  left: 0;
  bottom: 6px;
  margin-left: 6px;
  margin-right: 5px;
}

cover-view {
  display: block;
}

.houses cover-image {
  width: 100%;
  height: 170px;
}

