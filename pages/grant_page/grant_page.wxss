  flex-wrap: wrap;
  display: block;
  font-size: 16px;
  color: #444;
  margin-left: 10px;
  margin-top: 10px;
}

.sp {
  padding-left: 10px;
}

.houses .sp cover-view {
  font-size: 12px;
